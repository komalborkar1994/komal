		this.drname = drname;
	}


	public String getDrdeparment() {
		return drdeparment;
	}


	public void setDrdeparment(String drdeparment) {
		this.drdeparment = drdeparment;
	}


	public String getDreducation() {
		return dreducation;
	}


	public void setDreducation(String dreducation) {
		this.dreducation = dreducation;
	}


	{
	
	}
	

}
