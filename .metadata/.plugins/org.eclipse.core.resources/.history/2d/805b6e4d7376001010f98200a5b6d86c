package staticprogram;

public class Animal1 {
	static  String name;
	static int age;
	static String color;
	static {
		System.out.println("hiii i am pet animal");
	}
	static void getDetail() {
		System.out.println("name:"+ "buzo");
		System.out.println("age:"+14 );
		System.out.println("color:"+ "black");
	}
	
	

}
